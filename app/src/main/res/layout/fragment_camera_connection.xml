    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".objectdetekt.presentation.view.CameraConnectionFragment">

    <com.eniskaner.eyojtfcase.objectdetekt.presentation.customview.AutoFitTextureView
